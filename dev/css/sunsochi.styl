		top 50%
		left 50%

		display block
		width 88px
		height 88px

		padding 10px
		box-sizing border-box

		transform translate(-50%,-50%)

		background $colorBlue
		opacity 0.8

		border-radius 50%
		text-align center

		box-shadow: 0 0 0 rgba(#f99d22, 0.4)

		animation pulse 1.5s infinite

		z-index -1

		&:hover
			animation none

		img
			position relative
			left 50%

			display block
			width 100%
			height 100%


			transform translateX(-50%)

			z-index 100

.body-preloader
	position fixed
	top 0
	left 0
	right 0
	bottom 0
	display block
	width 100%
	height 100%

	z-index 10000000

	background #fff

	transition opacity 0.3s $transitionAnimation

	.preloader-container
		position absolute
		top 50%
		left 50%

		display block
		width 88px
		height 88px

		padding 10px
		box-sizing border-box

		transform translate(-50%,-50%)

		background $colorBlue
		opacity 0

		border-radius 50%
		text-align center

		box-shadow: 0 0 0 rgba(#f99d22, 0.4)

		animation pulse 1.5s infinite

		z-index -1

		&:hover
			animation none

		img
			position relative
			left 50%

			display block
			width 100%
			height 100%


			transform translateX(-50%)

			z-index 100

