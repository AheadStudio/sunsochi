	&.preloader-destroy
		.preloader-bcg
			opacity 0
			z-index -1

.preloader-active
	position relative

.preloader-bcg
	position absolute
	top 0
	left 0
	right 0
	bottom 0

	display block
	width 100%
	height 100%

	z-index 10

	transition all 0.3s $transitionAnimation

	.preloader-container
