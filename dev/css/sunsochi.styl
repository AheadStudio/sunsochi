.apartment-progress
	padding 40px 0

.apartment-progress-text
	display block
	margin-bottom 25px

	font-size 18px
	font-family "ProximaNovaBold"

.apartment-progress-bar
	position relative

	margin-bottom 20px
	width 100%
	height 10px

	background $colorTurquoise

	.apartment-progress-bar-fill
		position absolute
		left 0
		top 0
		bottom 0

		display block
		width 0
		height 100%

		background $colorBlue

.apartment-progress-list
	display flex
	justify-content space-between

	.apartment-progress-item
		font-size 14px

		&.active
			color $colorBlue
			font-family "ProximaNovaSemibold"


/*** //apartment progress ***/


/*** options ***/

.options
	padding 30px 0 50px 0
	box-sizing border-box

	h2
		margin-bottom 55px

.options-list
	display flex
	width 100%
	flex-wrap wrap
	justify-content flex-start
	align-items baseline

	&--2
		.options-item
			width 49%
			margin-right 2%

			&:nth-child(2n)
				margin-right 0
	&--3
		.options-item
			width 30%
			margin-right 5%

			&:nth-child(3n)
				margin-right 0
	&--4
		.options-item
			width 24%
			margin-right 2%

			&:nth-child(4n)
				margin-right 0

.options-item
	position relative
	min-width 200px
	padding 0 0 30px 0
	box-sizing border-box

	.options-container
		position relative
		width 100%
		margin-bottom 25px

	.options-img
		display block
		width 100%
		height 100%

		object-fit cover

	.options-title
		margin-bottom 25px
		font-size 20px
		color #333333

		font-family "ProximaNovaSemibold"

	.options-text
		font-size 16px
		line-height 22px
		color #4A494A

	.options-more
		position absolute
		top 50%
		left 50%

		max-width 170px
		opacity 0

		text-align center

		transform translate(-50%,-60%)
		transition transform 0.3s $transitionAnimation, opacity 0.3s $transitionAnimation

		z-index 20

	&.options-item--hover
		box-shadow 0px 0px 22px -2px transparent

		transition box-shadow 0.3s $transitionAnimation

		&:before
			content ""
			position absolute
			top 0
			left 0
			bottom 0
			right 0

			display block
			width 100%
			height 100%

			transform scale(0.8)
			background rgba(255, 255, 255, 0.95)

			opacity 0
			transition transform 0.3s $transitionAnimation, opacity 0.3s $transitionAnimation

			z-index 10

	.options-icon
		display block
		max-width 25px
		height 25px
		width 100%

		svg
			display block
			width 100%
			min-height 100%

			transition transform 0.3s $transitionAnimation

	&:hover
		&.options-item--hover
			box-shadow 0px 0px 30px -5px rgba(0,0,0,0.75)

			&:before
				transform scale(1)
				opacity 1

		.options-more
			opacity 1
			transform translate(-50%,-50%)


/*** //options ***/


/*** investments-info ***/

.investments-info
	padding 30px 0 100px 0
	box-sizing border-box

	h2
		max-width 725px
		font-size 28px
		line-height 43px
		margin 0 auto 60px auto
	p
		line-height 26px

	.options-list
		margin-top 60px

.investments-offers
	padding 40px 0

	h2
		margin-bottom 20px
/*** //investments-info ***/


/*** employees ***/

.employees
	padding 0 0 110px 0

	.employees-section
		display block
		margin-bottom 75px

		font-size 20px
		font-family "ProximaNovaSemibold"

		color #333333
		text-align center

.employees-list
	display flex
	justify-content flex-start
	flex-wrap wrap

.employees-item
	display flex
	justify-content space-between
	flex-wrap wrap
	margin-bottom 90px

	width 45%
	margin-right 7%

	&:nth-child(2n)
		margin-right 0

	.employees-container
		width 50%
		padding-right 50px
		box-sizing border-box

	.employees-img
		display block
		width 100%
		height 100%

		object-fit cover

	.employees-info
		width 50%

	.employees-name
		margin 5px 0 7px 0

		font-family "ProximaNovaSemibold"
		font-size 18px

	.employees-position
		margin 0 0 15px 0

		font-size 18px
		line-height 25px

	.employees-contact
		display inline-block
		margin-bottom 10px

/*** //employees ***/


/*** vacancies ***/

.vacancies
	margin-bottom 100px
	.page-title
		margin-bottom 70px

.vacancies-container
	display flex
	justify-content space-between
	flex-wrap wrap

.vacancies-list
	width 66%
	padding 50px 55px
	box-sizing border-box

	background $colorLightBlue

.vacancies-item
	margin-bottom 25px

	.vacancies-item-title
		margin-bottom 40px

		font-size 25px
		font-family "ProximaNovaSemibold"

	.vacancies-item-subtitle
		margin 50px 0 30px 0
		font-size 20px
		font-family "ProximaNovaSemibold"

		color #333333

	.vacancies-link
		margin-top 50px
		max-width 270px

		text-align center

.vacancies-contacts
	width 34%
	padding 50px 0 0 40px
	box-sizing border-box

	.vacancies-subtitle
		margin-bottom 20px
		font-size 20px
		font-family "ProximaNovaSemibold"
		color #333333

	.vacancies-link
		display inline-block
		margin-bottom 20px
		font-size 18px
		color #4A494A
/*** //vacancies ***/


/*** about ***/
.about
	h1
		margin 55px 0 50px 0

	p
		font-size 16px
		color #4A494A

.about-info
	margin 65px 0 35px 0
	padding 50px 0 95px 0

	background-image url(../svg/about_icon_all.svg)
	background-repeat no-repeat
	background-size 1120px
	background-position center 8px

	h2
		margin-bottom 20px

	.about-info-container
		position relative
		min-height 480px
		.about-info-item
			position absolute
			display block
			transform translateY(-20px)

			opacity 0

			transition opacity 0.3s $transitionAnimation, transform 0.3s $transitionAnimation

			z-index 10
			&.active
				opacity 1
				transform translateY(0)

			.about-info-title
				display block

				font-size 64px
				font-family "ProximaNovaLight"

				color #009DCC


			.about-info-text
				display block

				font-size 20px
				font-family "ProximaNovaSemibold"

				line-height 25px
				color #009DCC

			&:nth-child(1)
				top 0px
				left 320px

			&:nth-child(2)
				top 105px
				right 10px

			&:nth-child(3)
				top 62px
				left 75px

			&:nth-child(4)
				top 230px
				left 415px

			&:nth-child(5)
				top 255px
				left 20px

			&:nth-child(6)
				top 395px
				left 265px

			&:nth-child(7)
				top 365px
				right 60px

/*** //about ***/


/*** news **/

.news
	h1
		margin-bottom 95px

.news-list
	.news-item
		display flex
		justify-content space-between
		margin-bottom 55px
		transition transform 0.3s $transitionAnimation, opacity 0.3s $transitionAnimation

		&.load-events-item
			transform translateY(-30px)
			opacity 0

	.news-holder
		display block
		width 50%

		overflow hidden

		.news-img
			display block
			width 100%
			height 100%
			object-fit cover
			transition transform 0.3s $transitionAnimation

		&:hover
			.news-img
				transform scale(1.1)

	.news-info
		display block
		width 50%
		padding 45px 50px 25px 35px
		box-sizing border-box
		background-color $colorLightBlue

	.news-date
		margin-bottom 20px
		font-size 14px
		color #4A494A

	.news-title
		font-size 20px
		font-family "ProximaNovaSemibold"
		line-height 30px
		color #333333

	.news-text
		height 115px
		margin-top 15px

		font-size 16px

		line-height 22px
		overflow hidden

		color #4A494A

.news-more
	margin-bottom 60px


.news-detail
	h1
		margin-bottom 40px
		text-align left
		line-height 53px

	.news-detail-date
		margin-bottom 50px

	.news-detail-subtitle
		margin 50px 0 20px 0
		font-size 20px
		font-family "ProximaNovaSemibold"
		color #333333

	p
		font-size 16px
		margin-bottom 25px

	.new-detail-info
		transition transform 0.3s $transitionAnimation, opacity 0.3s $transitionAnimation

		&.load-events-item
			transform translateY(-30px)
			opacity 0

.news-detail-more
	text-align left
	margin 100px 0 120px 0

	.load-more
		margin 0

/*** //news **/


/*** reviews ***/

.reviews
	h1
		margin-bottom 85px

.reviews-list
	margin-bottom 100px

.reviews-item
	position relative

	display flex
	justify-content flex-end
	margin-bottom 110px
	transition transform 0.3s $transitionAnimation, opacity 0.3s $transitionAnimation

	&.load-events-item
		transform translateX(-30px)
		opacity 0

	.reviews-video
		position absolute
		top 50px
		left 0

		display block
		max-width 50%
		width 100%

		height 320px

		z-index 10

	.reviews-container
		position relative
		max-width 60%
		width 100%

		padding 40px 60px 40px 15%
		border 2px solid $colorOrange
		box-sizing border-box

		.reviews-icon
			width 65px
			margin-bottom 30px

			svg
				display block
				width 100%

				path
					fill $colorLightBlue
					stroke $colorLightBlue

		.reviews-name
			margin-bottom 15px
			font-size 16px
			color #000

			.reviews-name-lastname
				font-family "ProximaNovaSemibold"

		.reviews-text
			margin-bottom 25px

			font-size 16px
			line-height 24px

		&:after
			content ""
			position absolute
			bottom -52px
			left 10%
			display block
			width 0
			height 0
			border-top 52px solid $colorOrange
			border-right 52px solid transparent
			z-index 5

		&:before
			content ""
			position absolute
			bottom -47px
			left 10.4%
			display block
			width 0
			height 0
			border-top 47.5px solid #fff
			border-right 48px solid transparent
			z-index 6

	&:nth-child(2n)
		flex-flow row
		justify-content flex-start

		.reviews-video
			left auto
			right 0

		.reviews-container
			padding 40px 15% 40px 60px

			&:after
				right 10%
				left auto
				border-top 52px solid $colorOrange
				border-left 52px solid transparent
				border-right none

			&:before
				right 10.4%
				left auto
				border-top 47.5px solid #fff
				border-left 48px solid transparent
				border-right none


.reviews-more
	text-align center
	margin-bottom 100px

	.load-more
		max-width 215px
		margin 0 auto
		text-align center

.toggle-container
	display none
	opacity 0
	transform translateX(-50px)
	transition transform 0.3s $transitionAnimation, opacity 0.3s $transitionAnimation

	&.active
		display block

	&.active-animation
		opacity 1
		transform translateX(0)
/*** //reviews ***/


/*** developers ***/

.developers
	margin-bottom 100px

	h1
		margin-bottom 45px

	.page-subtitle
		max-width 570px
		margin 0 auto
		line-height 26px

	.developers-list
		display flex
		justify-content flex-start
		flex-wrap wrap
		align-items flex-start

		margin-top 50px

	.developers-item
		display block
		width 32%
		margin 0 2% 60px 0
		min-width 300px

		&:nth-child(3n)
			margin-right 0

	.developers-letter
		margin-bottom 10px
		font-size 64px
		font-family "ProximaNovaLight"

		color $colorBlue

	.developers-link
		display table
		width auto
		margin-bottom 12px
		color #4A494A

		&:hover
			color $colorBlue

	.developer-info
		.img
			width 100%
			max-width 225px

		p.right
			padding-left 280px
			box-sizing border-box

.developers-offers
	padding 60px 0 70px 0

/*** //developers ***/


/*** partners ***/

.partners
	margin-bottom 180px

	h1
		margin-bottom 80px

	.partners-list
		display flex
		flex-wrap wrap
		justify-content flex-start

	.partners-item
		width 48%
		min-width 250px
		margin-right 4%
		margin-bottom 130px

		&:nth-child(2n)
			margin-right 0

	.partners-holder
		display block
		width 70%
		max-height 130px
		margin-bottom 70px

	.partners-img
		display block
		max-height 150px

	.partners-title
		margin-bottom 15px
		font-size 20px
		font-family "ProximaNovaSemibold"

		color #333333

	.partners-text
		font-size 16px
		line-height 24px
		color #4A494A

/*** //partners ***/


/*** blog ***/

.blog
	padding-bottom 60px

	h1
		margin-bottom 45px

	.blog-search
		margin-bottom 60px
		padding 40px 40px
		box-sizing border-box

		background $colorLightBlue

	.form
		margin-top 0

	.form-row
		margin-bottom 0

	.blog-info
		display flex
		flex-wrap wrap
		justify-content space-between

		margin-bottom 35px

	.blog-container
		width 66%

	.blog-holder
		display block
		width 100%
		margin-bottom 50px

		overflow hidden

		.blog-img
			display block
			width 100%
			height 100%

			transition transform 0.3s $transitionAnimation


	.blog-title
		display block
		margin-bottom 20px

		font-size 25px
		font-family "ProximaNovaSemibold"

	.blog-link
		&:hover
			.blog-img
				transform scale(1.1)
			.blog-title
				color $colorOrange
	.blog-data
		display inline-block
		margin 0 10px 25px 0
		vertical-align middle

		font-size 14px
		color #4A494A

	.blog-views
		display inline-block
		vertical-align middle
		margin 0 10px 25px 0

		.blog-views-icon
			display inline-block
			width 35px
			height 20px
			margin-right 5px
			float left

			svg
				display block
				width 100%

		&.active
			.blog-views-icon
				svg
					path
						fill $colorOrange

		.blog-views-count
			display inline-block
			margin-top 5px
			font-size 13px
			font-family "ProximaNovaSemibold"
			color #4A494A

	.blog-rating
		display inline-block
		margin 0 10px 25px 0

		vertical-align middle

		img
			display inline-block
			width 20px
			height 20px

			margin-right 5px

			&:last-child
				margin-right 0

	.blog-text
		margin-bottom 25px
		font-size 16px
		line-height 22px
		color #4A494A

	.blog-tags
		display flex
		justify-content flex-start
		align-items center
		flex-wrap wrap

		margin-bottom 30px

		.blog-tag
			margin 0 10px 10px 0
			padding 10px 25px
			box-sizing border-box

			border-radius 25px

			text-align center
			background #eceded

			font-size 13px
			font-family "ProximaNovaSemibold"

			color #4A494A

			cursor pointer

	.blog-topics
		width 30%
		padding 40px 20px
		box-sizing border-box

		background $colorLightBlue

		p
			margin-bottom 35px

	.blog-topics-title
		margin-bottom 35px
		font-family "ProximaNovaSemibold"

	.blog-topics-item
		line-height 20px
		color #009DCC
		font-family "ProximaNovaSemibold"

	.blog-list
		display flex
		justify-content flex-start
		flex-wrap wrap
		align-items flex-start

		box-sizing border-box
		margin-bottom 50px
		border-bottom 1px solid $colorTurquoise

	.blog-item
		width 48%
		min-width 280px

		margin 0 4% 55px 0
		transition transform 0.3s $transitionAnimation, opacity 0.3s $transitionAnimation

		&:nth-child(2n)
			margin-right 0

		&.load-events-item
			transform translateY(-30px)
			opacity 0

		.blog-holder
			margin-bottom 30px

	&.blog-detail
		h1
			text-align left
			margin-bottom 45px

		h3
			margin-bottom 20px
			font-size 20px
			color #333333

		p
			font-size 16px
			line-height 22px
			color #4A494A

	.blog-actions
		display flex
		justify-content space-between
		align-items center
		flex-wrap wrap

		margin-bottom 30px

		padding 25px 0
		box-sizing border-box

		border-top 1px solid $colorTurquoise
		border-bottom: 1px solid $colorTurquoise

		.blog-share
			display flex
			justify-content flex-start
			align-items center

			.blog-share-title
				margin-right 10px

				font-size 15px
				font-family "ProximaNovaSemibold"

			.blog-share-items

				.blog-share-item
					display inline-block
					width 25px
					height 25px

					margin-right 10px

					cursor pointer
					svg
						display block
						width 100%

						path
							fill $colorGray
							fill-opacity 1
							transition fill 0.3s $transitionAnimation

					&:last-child
						margin-right 0

					&:hover
						svg
							path
								fill $colorOrange

		.blog-estimate
			display flex
			justify-content flex-start
			align-items center

			.blog-estimate-title
				margin-right 20px

				font-size 15px
				font-family "ProximaNovaSemibold"

			.blog-rating
				img
					width 25px
					height 25px
					margin-right 8px

.blog-offers
	padding 60px 0 0 0
	box-sizing border-box

	h2
		margin-bottom 50px

	.blog-list
		margin-bottom 0
		padding-bottom 0
		border none

	.blog-tags
		.blog-tag
			background #fff

/*** //blog ***/


/*** contacts ***/

.contacts
	h1
		margin-bottom 65px

	.map-container
		height 565px
		margin-bottom 0

	.contacts-holder
		width 100%
		margin-bottom 85px

		.contacts-img
			display block
			width 100%

	.contacts-container
		display flex
		justify-content flex-start
		align-items baseline

		flex-wrap wrap

		margin-bottom 100px

		.contacts-item
			width 48%
			min-width 280px

			margin-right 4%

			&:last-child
				margin-right 0

			p
				display block
				margin-bottom 20px

		.contacts-title
			margin-bottom 15px
			font-size 20px
			font-family "ProximaNovaSemibold"

			color #333333

/*** //contacts ***/


/*** 404 ***/

.page-404
	position relative

	height 100vh
	padding 95px 0 40px 0
	box-sizing border-box
	text-align center

	.page-404-logo
		display block
		max-width 240px
		margin 0 auto 35px auto

		svg
			display block
			width 100%

	.page-404-desc
		font-size 14px
		margin-bottom 70px

	.page-404-icon
		max-width 74px
		margin 0 auto 50px auto

		svg
			width 100%

	.page-404-title
		margin-bottom 50px

	.page-404-back
		font-size 16px
		line-height 28px
		color #4A494A

	.page-404-copyrights
		position absolute
		bottom 40px
		left 50%

		transform translateX(-50%)
/*** //404 ***/


/*** form-popup ***/

.form-popup
	h2
		font-size 28px
		margin-bottom 30px

	h3
		display block
		max-width 400px
		margin 0 auto

		font-size 18px
		font-family "ProximaNovaRegular"

		text-align center

	.form-order-call
		display block
		width 100%
		max-width 420px
		margin 0 auto

		padding-left 0

		.send-order
			margin 0 auto
			font-size 18px
			font-family "ProximaNovaSemibold"
	.order-call-icon
		position inherit
		display block
		max-width 155px
		width 100%

		margin 20px auto 40px auto

		svg
			path:not([fill="none"])
				fill $colorBlue

	.order-call-privacy
		line-height 20px
		.link
			display inline-block
			color $colorBlue


.mfp-blue
	.form-popup
		h2
			color #fff
		h3

		.order-call-icon
			svg
				path:not([fill="none"])
					fill #fff

		.order-call-privacy
			line-height 20px
			.link
				display inline
				color #fff

	.btn-container-close
		position absolute
		top 30px
		right 30px

		display block
		width 20px
		height 20px

		svg
			display block
			width 100%

			path
				transition fill 0.3s $transitionAnimation
				fill #fff

		&:hover
			svg
				path
					fill $colorLightBlack

/*** //form-popup ***/


/*** autocomplete ***/

.autocomplete-suggestions
	padding 0 20px 20px 20px
	box-sizing border-box

	border 1px solid $colorLightGray

	border-bottom-left-radius 5px
	border-bottom-right-radius 5px

	background #fff

	.autocomplete-suggestion
		margin-top 20px
		cursor pointer

	.autocomplate-item
		font-size 16px
		color $colorLightBlack

		transition color 0.3s $transitionAnimation

		&:hover
			color #e3e3e3

	b
		font-family "ProximaNovaSemibold"

/*** //autocomplete ***/


/*** youtube video ***/

.fake-preview
	position relative
	display block
	width 100%
	height 100%

	background-image url(../dummy/youtube/preview_youtube.png)
	background-repeat no-repeat
	background-size cover

	cursor pointer

	&:before
		content ""
		position absolute
		top 0
		left 0
		right 0
		bottom 0

		display block
		width 100%
		height 100%

		background-color rgba(0, 0, 0, 0.5)

		z-index 10

	.fake-preview-icon
		position absolute
		top 50%
		left 50%

		display block
		width 85px
		height 85px

