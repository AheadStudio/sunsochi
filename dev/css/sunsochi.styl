/*** Settings ***/
@import "nib"
@import "../lib/css/css"

global-reset();
reset-html5();
form-reset();
b
strong
i
em
	font-style normal
	font-weight normal

$transitionAnimation = cubic-bezier(0.250, 0.460, 0.450, 0.940)

$colorDarkGray = #3a4347
$colorGray = #9b9b9b
$colorLightBlue = #f7f8fb
$colorLightGray = #e3e3e3
$colorTurquoise = #e1e8eb
$colorLightBlack = #2C2F2F
$colorOrange = #f99d22
$colorRed = #f01624
$colorBlue = #009ecc

/* fonts */
@font-face
	font-family "ProximaNovaBlack"
	src url("../fonts/ProximaNova-Black.woff") format("woff")

@font-face
	font-family "ProximaNovaLight"
	src url("../fonts/ProximaNova-Light.woff") format("woff")

@font-face
	font-family "ProximaNovaRegular"
	src url("../fonts/ProximaNova-Regular.woff") format("woff")

@font-face
	font-family "ProximaNovaSemibold"
	src url("../fonts/ProximaNova-Semibold.woff") format("woff")

@font-face
	font-family "ProximaNovaBold"
	src url("../fonts/ProximaNova-Bold.woff") format("woff")

@font-face
	font-family "Rubl"
	src url("../fonts/rouble.woff") format("woff")
/* //fonts */

html
	height 100%
body
	position relative
	width 100%
	height 100%
	min-height 100vh
	line-height 1
	cursor default
	font-family "ProximaNovaRegular"

	color $colorLightBlack

	transition opacity 0.3s $transitionAnimation

/*** //Settings ***/

/*** main style ***/
.page
	display block
	width 100%

	min-height 100%

	margin 0 auto

.page-inner
	position relative
	display block
	margin 0 auto
	box-sizing border-box

	&--w1
		max-width 1000px
		padding 0 20px

	&--w2
		max-width 810px
		padding 0 20px

	&--w3
		max-width 750px
		padding 0 20px

	&--w4
		max-width 655px
		padding 0 20px

	&--max
		max-width 100%

/** header **/
.page-header
	width 100%

.header-row-items
	display flex
	justify-content  space-between
	align-items center

.header-row--top
	width 100%
	margin-bottom 45px
	background-color $colorDarkGray

.header-top
	padding 15px 0

	.header-top-item-holder
		display inline-block
		margin-right 40px

		&:last-child
			margin-right 0

		.header-top-item
			font-size 14px
			font-family "ProximaNovaSemibold"

			&.active
				color $colorOrange

.header-logo
	display flex
	max-width 460px

	.header-logo-img
		display inline-block
		margin-right 20px
		float left
		max-width 215px
		width 100%

		svg
			display block
			width 100%

	.header-logo-title
		display block
		width 50%
		padding-top 4px
		vertical-align top

		box-sizing border-box

		font-size 14px

	&.header-logo--mini
		display none
		width 25px

		margin-right 0
		height 100%

		.header-logo-img
			width 100%

			svg
				path
					transition fill 0.3s $transitionAnimation

		&:hover
			.header-logo-img
				svg
					path
						fill $colorOrange

.header-mobile
	.header-mobile-title
		display block
		margin-bottom 10px

		font-size 14px
		color $colorBlue

	.header-mobile-item
		display block

		font-size 22px
		font-family "ProximaNovaBold"

		color $colorBlue

		white-space nowrap

.header-row-items
	.header-call
		padding 15px 25px
		font-size 14px
		max-height 47px

		white-space nowrap

.header-row--middle
	padding-bottom 20px

.header-bottom-inner
	.header-bottom-item-holder
		display inline-block
		margin-right 35px

		&:last-child
			margin-right 0

		.header-bottom-item
			font-size 16px

			font-family "ProximaNovaSemibold"

			&.active
				color $colorOrange

.header-favorites
	display flex
	align-items center

	.header-favorites-holder
		display block
		width 25px
		height 25px

		svg
			display block
			width 100%

			path
				transition fill 0.3s $transitionAnimation

		&.active
			svg
				path
					fill $colorOrange

	.header-favorites-count
		padding-left 10px
		box-sizing border-box
		font-size 15px
		font-family "ProximaNovaSemibold"

		color $colorLightBlack

.header-row--bottom
	padding 15px 0
	border-top 2px solid $colorLightBlue
	border-bottom 2px solid $colorLightBlue

.fixed-header
	.header-row--middle
		padding-bottom 88px

	.header-row--bottom
		position fixed
		top 0
		left 0
		right 0

		width 100%
		padding 10px 0

		background #fff
		border-bottom 3px solid $colorLightGray

		opacity 0
		transform translateY(-100px)
		transition transform 0.3s $transitionAnimation

		z-index 1010

		.header-logo
			&.header-logo--mini
				display block

	&.fixed-header--show
		.header-row--bottom
			opacity 1
			transform translateY(0)
/** //header **/


/** //mobile **/

.header-burger
	position relative
	display none
	width: 20px
	height 14px
	cursor pointer

	transition transform 0.3s $transitionAnimation

	.header-burger-inner
		position absolute
		top 6.5px
		left 0
		right 0

		display block
		width 100%
		height 2px

		background-color #4a4a4a
		transition opacity 0.3s $transitionAnimation

	&:before
	&:after
		content ""
		position: absolute

		left 0
		right 0

		display block
		width 100%
		height 2px

		background-color #4a4a4a

		transition transform 0.3s $transitionAnimation, background-color 0.3s $transitionAnimation

	&:before
		top 0

	&:after
		bottom 0

	&.active
		z-index 1010

	&.active--hover
		.header-burger-inner
			opacity 0
			background #fff
		&:before
			background #fff
			transform rotate(-40deg) translateX(-3px) translateY(6.5px)
		&:after
			background #fff
			transform rotate(40deg) translateX(-2px) translateY(-5px)

.header-mobile-elements
	display none

.mobile-menu
	position: fixed
	left 0
	right 0
	top 0
	bottom 0
	display none
	z-index 1000

	&.active-block
		display block
		overflow-y auto

body.open-menu
	position fixed
	top 0
	left 0
	right 0
	bottom 0
	overflow hidden
	height 100%
	-webkit-overflow-scrolling touch

	.mfp-wrap
		bottom 0
		overflow-y auto

	.header-logo
		position relative
		z-index: 10000

		svg
			path[fill='#009ecc']
				fill #fff

			path[fill='#f89d21']
				fill $colorOrange

	.page-header
		position relative
		background-color $colorBlue

		z-index 1001

		.mobile-favorites
			opacity 0


.mobile-menu
	.mobile-menu-background
		position fixed
		top 0px
		right 0
		left 0
		bottom 0
		display block
		width 100%
		height 100%
		background #009ecc
		transform translateX(-100%)
		opacity 0
		transition transform 0.6s $transitionAnimation, opacity 0.3s $transitionAnimation
		transition-delay 0.3s

	.mobile-menu-container
		position relative
		display none
		height auto
		margin-top: 100px
		padding-bottom 50px
		box-sizing border-box
		transform translateX(-50px)
		transition transform 0.3s $transitionAnimation, opacity 0.3s $transitionAnimation
		transition-delay 0s
		opacity 0

.mobile-menu
	&.active-block
		.mobile-menu-container
			display block

	&.active-show
		.mobile-menu-background
			transform translateX(0)
			opacity 1
			transition-delay 0s

		.mobile-menu-container
			transform translateY(0)
			opacity 1
			transition-delay 0.4s


.mobile-menu-container
	.mobile-menu-title
		padding 0 20px
		box-sizing border-box
		margin-bottom 30px
		font-size 18px
		color #fff

	.header-mobile-title
		display block
		padding 0 20px
		box-sizing border-box

		margin-bottom 10px
		color #fff

	.header-mobile-item
		padding 0 20px
		box-sizing border-box

		font-size 24px
		font-family "ProximaNovaBold"

		color #fff

		&:hover
			color $colorOrange

	.header-call-mobile
		padding 15px
		margin 20px 0 0 20px
		max-width 165px

	.mobile-menu-top
		margin-top 20px
		padding 20px 20px 0 20px
		border-top 1px solid $colorLightGray

		.mobile-menu-top-item-holder
			margin-bottom 30px

			&:last-child
				margin-bottom 0

			.mobile-menu-top-item
				font-size 20px
				font-family "ProximaNovaSemibold"

	.mobile-menu-bottom
		margin-top 20px
		padding  20px 20px 0 20px
		border-top 1px solid $colorLightGray

		.mobile-menu-bottom-item-holder
			margin-bottom 30px

			&:last-child
				margin-bottom 0

			.mobile-menu-bottom-item
				font-size 16px
				font-family "ProximaNovaSemibold"

/** //mobile **/


/** footer **/

.page-footer
	background $colorDarkGray
	padding 55px 70px 40px 70px

.footer-row
	display flex
	flex-wrap wrap
	justify-content space-between
	align-items center

	&--top
		margin-bottom 25px

	&--bottom
		margin-top 70px

.footer-top-item-holder
	display inline-block
	margin-right 20px

	&:last-child
		margin-right 0

	.footer-top-item
		font-size 16px
		font-family "ProximaNovaSemibold"

.footer-next-item-holder
	display inline-block
	margin-right 23px

	&:last-child
		margin-right 0

	.footer-next-item
		font-size 15px

.footer-info
	.footer-info-private
		display block
		margin-bottom 10px
		padding-bottom 1px
		color $colorOrange

		border-bottom 1px solid transparent
		transition border-bottom-color 0.3s $transitionAnimation

		font-size 15px

		&:hover
			border-bottom-color $colorOrange

	.footer-info-name
		display block
		margin-bottom 10px

		font-size 15px

		color #fff

	.footer-creators
		.footer-creators-title
			margin-right 5px
			color #fff
			font-size 15px
		.footer-creators-name
			color $colorOrange
			font-size 15px
			border-bottom 1px solid transparent
			transition border-bottom-color 0.3s $transitionAnimation
		&:hover
			.footer-creators-name
				border-bottom-color $colorOrange

.footer-social
	display grid
	grid-template-columns 1fr 1fr 1fr 1fr
	grid-gap 0 20px

	.footer-social-item
		display block
		width 25px
		height 25px

		border-radius 7px
		overflow hidden

		cursor pointer

		transition opacity 0.3s $transitionAnimation

		svg
			display block
			width 100%
			path[fill="#fff"]
				fill-opacity 1
				transition fill 0.3s $transitionAnimation, fill-opacity 0.3s $transitionAnimation

		&:hover
			svg
				path[fill="#fff"]
					fill #fff
					fill-opacity 0.5

.page-text
	font-size 16px
	font-family "ProximaNovaRegular"

	h1
		display block
		text-align center

		font-family "ProximaNovaSemibold"
		font-size 28px

		line-height 45px

	h2
		display block
		text-align center

		font-family "ProximaNovaSemibold"
		font-size 24px

		line-height 54px

	h3
		font-size 16px
		font-family "ProximaNovaSemibold"

	dl
		display grid
		grid-template-columns 1fr 1fr
		grid-gap 20px 0
		overflow hidden

		dt
			position relative
			font-size 15px

		dd
			font-size 15px
			font-family "ProximaNovaSemibold"
			color #4a494a
			text-align right

		&.dark
			dd
			dt
				color $colorLightBlack

				&.title
					font-family "ProximaNovaSemibold"
					margin-bottom 30px

	p
		line-height 22px
		margin-bottom 30px

		font-size 16px
		font-family "ProximaNovaRegular"

		&:last-child
			margin-bottom 0

	.border-text
		box-sizing border-box

		&--full
			border-top 1px solid $colorLightGray
			padding 40px 0 50px 0
			border-bottom 1px solid $colorLightGray

		&--bottom
			padding 0 0 30px 0
			border-bottom 1px solid $colorLightGray

	sup
		font-size 12px
		vertical-align top

	table
		width 100%

		thead
			tr
				th
					padding 17px 0 15px 0
					box-sizing border-box
					background-color $colorTurquoise
					border-bottom 2px solid #d0d0d0

					font-family "ProximaNovaSemibold"

					color #000

		tr
			width 100%

			td
				padding 25px 0
				box-sizing border-box

				border-bottom 1px solid $colorLightGray

				color #000
				font-size 18px

				&:first-child
					padding-left 5px

				&:last-child
					padding-left 10px

			&.not-active
				opacity 0.4

	.img
		display block
		width 100%

		&-w50
			width 50%

		&-w30
			width 30%

		&--right
			float right
			margin 0 0 40px 60px

		&--left
			float left
			margin 0 60px 40px 0

	.check-list
		display flex
		flex-wrap wrap
		justify-content space-between

		.check-item
			position relative

			display block
			width 48%

			margin-bottom 35px

			padding-left 40px
			box-sizing border-box

			font-size 18px
			font-family "ProximaNovaSemibold"

			color #4A494A

			line-height 25px

			&:before
				content ""
				position absolute
				left 0
				top 5px

				display block
				width 25px
				height 25px

				background-image url("../svg/check_blue.svg")
				background-repeat no-repeat
				background-size contain

	ul
		display block

		li
			position relative
			display block
			padding-left 40px
			box-sizing border-box
			margin-bottom 20px

			&:before
				content "\2022"
				position absolute
				left 10px
				top -2px

	.page-title
	h1
		display block
		margin 55px 0 30px 0

		font-size 50px
		font-family "ProximaNovaBold"

		color #4A494A
		text-align center

	.page-subtitle
		margin-bottom 50px
		font-size 20px
		font-family "ProximaNovaSemibold"
		color #333333
		text-align center

	b
		font-family "ProximaNovaSemibold"

	strong
		font-family "ProximaNovaSemibold"

	i
		font-style italic

	em
		font-style italic

	ol
		list-style: circle;

		li
			margin-bottom 20px

	figure
		display block
		width 100%

	figcaption
		display block
		margin-top 10px
		font-size 14px

/** //footer **/

/** button and link **/
.link
	text-decoration none

.link-white
	color #fff

.link-black
	color $colorLightBlack

.link-blue
	color $colorBlue

.link-orange
	color $colorOrange

.link--orange
	transition all 0.3s $transitionAnimation

	&:hover
		color $colorOrange

.link--blue
	transition all 0.3s $transitionAnimation

	&:hover
		color $colorBlue

.link--opacity
	transition all 0.3s $transitionAnimation
	&:hover
		opacity 0.6

.link-blue--bottom
	border-bottom 1px solid transparent
	transition all 0.3s $transitionAnimation

	&:hover
		border-bottom-color $colorBlue

.link-blue--bottom-reverse
	border-bottom 1px solid transparent
	transition all 0.3s $transitionAnimation

	&:hover
		border-bottom-color $colorBlue

.link-black--bottom
	border-bottom 1px solid $colorLightBlack
	transition all 0.3s $transitionAnimation

	&:hover
		border-bottom-color transparent

.link-black--bottom-inverse
	border-bottom 1px solid transparent
	transition all 0.3s $transitionAnimation

	&:hover
		border-bottom-color $colorLightBlack

.link-black--bottom-blue
	border-bottom 1px solid $colorLightBlack
	transition all 0.3s $transitionAnimation

	&:hover
		border-bottom-color $colorBlue

.link-orange--bottom
	border-bottom 1px solid transparent
	transition all 0.3s $transitionAnimation

	&:hover
		border-bottom-color $colorOrange

.link-orange--bottom-reverse
	border-bottom 1px solid $colorOrange
	transition all 0.3s $transitionAnimation

	&:hover
		border-bottom-color transparent

.link-white--bottom
	border-bottom 1px solid #fff
	transition all 0.3s $transitionAnimation

	&:hover
		border-bottom-color transparent

.link-white--bottom-reverse
	border-bottom 1px solid transparent
	transition all 0.3s $transitionAnimation

	&:hover
		border-bottom-color #fff

.link--showdow-black-blue
	box-shadow inset 0 -1px $colorLightBlack, inset 0 -4px transparent
	transition box-shadow 0.3s $transitionAnimation, color 0.3s $transitionAnimation

	&:hover
		color $colorBlue
		box-shadow inset 0 -1px $colorBlue, inset 0 -4px transparent

.link--showdow-blue
	box-shadow inset 0 -1px transparent, inset 0 -4px transparent
	transition box-shadow 0.3s $transitionAnimation, color 0.3s $transitionAnimation

	&:hover
		box-shadow inset 0 -1px $colorBlue, inset 0 -4px transparent

.button
	display block
	padding 10px 15px
	box-sizing border-box

	border-radius 45px

	border 2px solid

.button--orange
	border-color $colorOrange
	color $colorOrange
	line-height 1

	transition color 0.3s $transitionAnimation, border-color 0.3s $transitionAnimation, background 0.3s $transitionAnimation

	&:hover
		background $colorOrange
		color #fff

.button--orange-reverse
	border-color $colorOrange
	background $colorOrange
	color #fff
	line-height 1

	transition color 0.3s $transitionAnimation, border-color 0.3s $transitionAnimation, background 0.3s $transitionAnimation

	&:hover
		background #ffffff
		color $colorOrange

.button--blue
	border-color $colorBlue
	color $colorBlue
	line-height 1

	transition color 0.3s $transitionAnimation, border-color 0.3s $transitionAnimation, background 0.3s $transitionAnimation

	&:hover
		background $colorBlue
		color #fff

.button--white
	border-color #fff
	color #fff
	line-height 1

	transition color 0.3s $transitionAnimation, border-color 0.3s $transitionAnimation, background 0.3s $transitionAnimation

	&:hover
		background #fff
		color $colorOrange

.button--orange-flood
	background $colorOrange
	color #fff
	line-height 1

	transition background 0.3s $transitionAnimation
	border-color $colorOrange

	&:hover
		background #e68f1a

/*** main style ***/

/*** block intro ***/
.intro
	position relative
	width 100%
	min-height 400px
	margin-bottom 35px

	&:after
		content ""
		position absolute
		top 0
		left 0
		right 0
		bottom 0

		background-color rgba(0, 0, 0, 0.4)
		z-index 1

	.intro-img
		position absolute
		left 0
		top 0
		right 0
		bottom 0

		display block
		width 100%
		height 100%

		z-index -1

		object-fit cover
		object-position bottom

	.intro-text
		position relative
		top 0
		bottom 0

		max-width 850px
		margin 0 auto

		padding 110px 0
		box-sizing border-box

		text-align center

		font-size 50px
		font-family "ProximaNovaBold"
		line-height 60px

		color #fff

		z-index 10

	.intro-advantages
		position relative
		left 50%

		display flex
		justify-content space-between

		transform translateX(-50%)
		max-width 870px

		margin-top -50px
		padding-bottom 130px

		z-index 10

	.intro-advantages-item
		display flex
		justify-content flex-start
		width 30%

		margin-right 3%

		opacity 0
		transform translateY(-50%)

		transition opacity 0.6s $transitionAnimation, transform 0.6s $transitionAnimation

		&.active
			opacity 1
			transform translateY(0%)

		&:last-child
			margin-right 0

		.intro-advantages-icon
			display inline-block
			width 25px
			min-width 25px

			margin-right 20px

			vertical-align top

			svg
				display block
				width 100%

				path
					fill #fff

		.intro-advantages-text
			display inline-block
			font-size 20px
			font-family "ProximaNovaSemibold"

			color #fff

			line-height 25px

		sup
			vertical-align top
			font-size 12px
			line-height 1
/*** //block intro ***/


/*** block filter home ***/
.filter
	position relative

	margin-top 10px
	margin-bottom 35px

	.filter-tabs
		padding-top 25px

		border-top 1px solid $colorTurquoise

		&--center
			text-align center

	.filter-clear
		position absolute
		right 0
		top -40px

		color #9B9A9B

		font-family "ProximaNovaSemibold"
		font-size 12px

		&.reset
			color $colorOrange

.filter-tabs
	margin-bottom 30px

	.filter-tab
		display inline-block
		margin-right 15px
		margin-bottom 5px

		padding 13px 20px
		box-sizing border-box

		border 1px solid $colorLightGray
		border-radius 100px

		font-family "ProximaNovaSemibold"
		font-size 15px

		cursor pointer

		transition border-color 0.3s $transitionAnimation, background 0.3s $transitionAnimation, color 0.3s $transitionAnimation

		&:hover
			border-color $colorBlue
			color $colorBlue

		&.active-tab
			border-color $colorBlue
			background $colorBlue
			color #fff

			&:hover
				background #fff
				color $colorBlue

		&:last-child
			margin-right 0

.filter-list
	.filter-item
		transition transform 0.3s $transitionAnimation, opacity 0.3s $transitionAnimation

		&.hide-block
			display none

		&.hide
			transform translateY(10px)
			opacity 0

.form-filter
	.filter-submit
		margin 30px auto 0 auto

		font-size 18px
		padding 13px 30px

	.filter-selected-regions-list
		display flex
		justify-content flex-start
		align-items center
		flex-wrap wrap
		border-bottom 1px solid $colorTurquoise

		.filter-selected-regions-item
			position relative
			display block
			margin-bottom 20px
			margin-right 20px

			padding 13px 20px
			box-sizing border-box

			border-radius 50px

			background #f6f7fa

			cursor pointer

			.form-item--checkbox
				display none
				opacity 0

			.selected-regions-item-text
				font-size 14px
				padding-right 25px

			.selected-regions-item-close
				position absolute
